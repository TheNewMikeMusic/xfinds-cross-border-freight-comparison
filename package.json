{
  "name": "xfinds",
  "version": "0.1.0",
  "description": "Open-source shopping agent aggregator for smarter cross-border purchases. Compare prices across multiple agents with a modern glassmorphic UI.",
  "keywords": [
    "shopping-agent",
    "price-comparison",
    "cross-border",
    "e-commerce",
    "nextjs",
    "typescript",
    "tailwindcss"
  ],
  "author": "Xfinds Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheNewMikeMusic/Xfinds.git"
  },
  "homepage": "https://xfinds.cc",
  "bugs": {
    "url": "https://github.com/TheNewMikeMusic/Xfinds/issues"
  },
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 8000",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "fix-images": "node fix-images-locally.js",
    "fix-images-new-folder": "node fix-images-to-new-folder.js",
    "process-images": "tsx scripts/process-images.ts",
    "deploy": "npm run build && echo 'Build completed successfully!'"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.0.0",
    "fuse.js": "^7.0.0",
    "jose": "^6.1.1",
    "lucide-react": "^0.400.0",
    "next": "^14.2.0",
    "next-intl": "^3.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "sharp": "^0.34.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.4.0",
    "zod": "^3.23.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.1.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "jsdom": "^27.1.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.0",
